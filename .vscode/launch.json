  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: dev.py",
      "type": "debugpy",
      "request": "launch",
      "program": "dev.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Python Debugger: pytest",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "args": [
        "-vv",
        "tests/test_result_submission.py::TestResultSubmission::test_dataframe_with_wrong_extension"
      ],
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}"
    }
  ]
}
